<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Pagination?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.POSController"
            prefHeight="400.0" prefWidth="600.0"
            fx:id="borderPane">

    <top>
        <MenuBar fx:id="menuBar">
            <menus>
                <Menu text="File">
                    <items>
                        <MenuItem onAction="#onMenuEndSession" text="Switch User"/>
                        <MenuItem onAction="#onMenuItemExit" text="Exit"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
    <center>
        <Pagination prefHeight="200.0" prefWidth="600.0" BorderPane.alignment="CENTER" fx:id="paginationPOS"/>
    </center>


</BorderPane>
