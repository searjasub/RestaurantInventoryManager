<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.AdministrativeController">
    <center>
        <Pagination prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" fx:id="pagination"/>
    </center>
    <top>
        <MenuBar fx:id="menuBar">
            <menus>
                <Menu text="File">
                    <items>
                        <MenuItem onAction="#onMenuEndSession" text="Switch User"/>
                        <MenuItem onAction="#onMenuItemExit" text="Exit" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
    <bottom>
        <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <items>
                <RadioButton onAction="#allShow" fx:id="radioAll" mnemonicParsing="false" prefHeight="21.0" prefWidth="73.0" text="All"/>
                <RadioButton fx:id="radioClockedIn" mnemonicParsing="false" prefHeight="21.0" prefWidth="112.0" text="Clocked In"/>
            </items>
        </ToolBar>
    </bottom>
</BorderPane>
